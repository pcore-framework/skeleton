{
    "name": "pcore/skeleton",
    "description": "PCore skeleton",
    "type": "project",
    "keywords": [
        "php",
        "pcore",
        "skeleton",
        "framework",
        "pcore-framework",
        "swoole",
        "fpm"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "pcore/http-server": "1.0.*",
        "pcore/config": "1.0.*",
        "pcore/aop": "1.0.*",
        "pcore/di": "1.0.*",
        "pcore/event": "1.0.*",
        "pcore/session": "1.0.*",
        "pcore/console": "1.0.*",
        "pcore/database": "1.0.*",
        "pcore/validator": "1.0.*",
        "pcore/redis": "1.0.*",
        "pcore/cache": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/Kernel/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    }
}